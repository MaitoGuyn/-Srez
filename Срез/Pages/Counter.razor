<div class="row">

    <div class="col-sm-6">
        <div class="card" style="width: 18rem;">
            <img class="card-img-top" src="https://www.allrecipes.com/thmb/fFW1o307WSqFFYQ3-QXYVpnFj6E=/1500x0/filters:no_upscale():max_bytes(150000):strip_icc()/48727-Mikes-homemade-pizza-DDMFS-beauty-4x3-BG-2974-a7a9842c14e34ca699f3b7d7143256cf.jpg" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">Card title</h5>
                <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                <button class="btn btn-primary" @onclick="AddPrice5">Add</button>
            </div>
        </div>
    </div>

</div>
@code {
    private int price1 = 1000;
    private int price2 = 1100;
    private int price3 = 1300;
    private int price4 = 1400;
    private int price5 = 1500;
    private int price6 = 900;
    private int TotalPrice;
    public string pizzaType;
    
    public void AddPizzaNames()
    {
        switch (pizzaType)
        {
            case "Margherita":
                pizzaType = "Margherita";
                break;
            case "Pepperoni":
                pizzaType = "Pepperoni";
                break;
            case "Vegetarian":
                pizzaType ="Vegetarian";
                break;
            case "4cheeze":
                pizzaType = "4cheeze";
                break;
            default:
                // Добавляем общее название для других типов пицц
                break;
        }
    }


    private void AddPrice()
    {
        TotalPrice += price1;
        AddPizzaNames();
    }
    private void AddPrice1()
    {
        TotalPrice += price2;
        AddPizzaNames();
    }
    private void AddPrice2()
    {
        TotalPrice += price3;
        AddPizzaNames();
    }
    private void AddPrice3()
    {
        TotalPrice += price4;
        AddPizzaNames();
    }
    private void AddPrice4()
    {
        TotalPrice += price5;
        AddPizzaNames();
    }
    private void AddPrice5()
    {
        TotalPrice += price6;
        AddPizzaNames();
    }
    
}
